{
  "gruvboxMaterial.colorfulSyntax": true,
  "gruvboxMaterial.italicKeywords": true,
  "gruvboxMaterial.darkWorkbench": "material",
  "gruvboxMaterial.darkContrast": "hard",
  "gruvboxMaterial.darkCursor": "aqua",
  "gruvboxMaterial.darkPalette": "material",
  "gruvboxMaterial.darkSelection": "orange",
  "gruvboxMaterial.diagnosticTextBackgroundOpacity": "12.5%",
  // ================================================================================== Window settings
  "window.experimental.nativeContextMenuLocation": true,
  "workbench.iconTheme": "catppuccin-latte",
  "workbench.list.fastScrollSensitivity": 100,
  "workbench.list.smoothScrolling": true,
  "workbench.fontAliasing": "auto",
  "workbench.tree.expandMode": "singleClick",
  "workbench.tree.indent": 13,
  "workbench.tree.renderIndentGuides": "none",
  "workbench.tree.enableStickyScroll": false,
  "workbench.tree.stickyScrollMaxItemCount": 7,
  "workbench.editor.pinnedTabSizing": "compact",
  "workbench.editor.tabSizing": "fit",
  "workbench.settings.openDefaultKeybindings": true,
  "workbench.experimental.share.enabled": true,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "meta.function-call",
          "meta.type.annotation",
          "meta.method.declaration",
          "meta.name.function",
          "support.type.primitive",
          "accessor",
          "comment",
          "keyword.control",
          "keyword.operator.new",
          "keyword.operator.assignment",
          "keyword.operator.arithmetic",
          "storage.modifier",
          "storage.type",
          "support.class",
          "support.constant",
          "support.other",
          "support.type.object.module",
          "entity.name.method",
          "entity.name.section",
          "entity.name.selector",
          "entity.name.function",
          "entity.name.type",
          "entity.other",
          "entity.other.attribute-name",
          "entity.other.inherited-class",
          "variable.other.object",
          "meta.use.rust",
          "meta.function.definition.rust",
          "meta.function.call.rust",
          "meta.type.annotation",
          "entity.name.type.go",
        ],
        "settings": {
          // "fontStyle": "italic"
        }
      },
      {
        "scope": [
          "storage.type.function.arrow",
          "meta.paragraph.markdown",
          "entity.name.type.result.rust",
          "source.rust",
          "other",
        ],
        "settings": {
          // "fontStyle": "italic"
        }
      },
      {
        "scope": [
          // "entity.name.function",
          "variable.object.property",
          "entity.name.type",
          "variable.other.property",
          "meta.definition.variable",
          "meta.definition.function",
          "meta.function.expression",
          "variable.other.readwrite",
          "keyword.operator.namespace.rust",
          // "entity.name.type.rust",
          "variable.other.rust",
          "variable.other",
          "variable.parameter",
          "variable.other.object",
          "entity.name.namespace.rust",
          "entity.name.function.rust",
          "string",
          "entity.name.function.go",
          "constant.numeric",
        ],
        "settings": {
          "fontStyle": ""
        }
      },
    ]
  },
  "workbench.colorCustomizations": {
    "editorBracketHighlight.foreground1": "#e466d9",
    "editorBracketHighlight.foreground2": "#ff9300",
    "editorBracketHighlight.foreground3": "#0096ff",
    "editorBracketHighlight.foreground4": "#ff7e79",
    "editorBracketHighlight.foreground5": "#b3bd2e",
    "editorBracketHighlight.foreground6": "#51cbe9",
    "editorBracketHighlight.unexpectedBracket.foreground": "#ff0000",
    // "tree.indentGuidesStroke": "#ff7e79",
