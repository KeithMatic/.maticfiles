  "newFile.expandBraces": true,
  "newFile.defaultBaseFileName": "keithmatic",
  "newFile.defaultFileExtension": ".ts",
  "newFile.fileTemplates": {},
  "newFile.relativeTo": "file",
  "newFile.rootDirectory": "~",
  "newFile.showPathRelativeTo": "project",
  "newFile.useFileTemplates": true,
  // ================================================================================== Code Runner
  "code-runner.clearPreviousOutput": true,
  "code-runner.showExecutionMessage": false,
  "code-runner.enableAppInsights": true,
  "code-runner.preserveFocus": true,
  "code-runner.showRunIconInEditorTitleMenu": false,
  "code-runner.cwd": "",
  "code-runner.customCommand": "echo Hello",
  "code-runner.defaultLanguage": "",
  "code-runner.runInTerminal": true,
  "code-runner.executorMap": {
    "javascript": "node",
    // "python": "python3 -u",
    "python": "py -u", // using a python3 alias
    "go": "go run",
    "lua": "lua",
    // "rust": "cd $dir && rustc $fileName && $dir$fileNameWithoutExt",
    "rust": "cd $dir && cargo run -q $fileName",
    "sass": "sass --style expanded",
    "scss": "scss --style expanded",
    "mojo": "mojo run",
    "php": "C:\\php\\php.exe",
  },
  // ================================================================================== Files Exclude
