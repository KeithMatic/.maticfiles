{
  /* ---------------------------------------------------------------------- */
  /*                         custom ui css                                  */
  /* ---------------------------------------------------------------------- */

  /* ---------------------------------------------------------------------- */
  /*                             Cursor                                     */
  /* ---------------------------------------------------------------------- */
  "catppuccin.accentColor": "mauve",
  "catppuccin.bracketMode": "rainbow",
  "catppuccin.colorOverrides": {
    "macchiato": {}
  },
  "catppuccin.customUIColors": {
    "macchiato": {
      "editor.lineHighlightBackground": "#1e2336",
      "editor.selectionHighlightBackground": "#1e2336",
      "editor.hoverHighlightBackground": "#1e2336",
      "list.focusBackground": "#1e2336",
      "list.activeSelectionBackground": "#1e2336",
      "list.hoverBackground": "#1e2336",
      "list.inactiveFocusBackground": "#1e2336",
      "list.inactiveSelectionBackground": "#1e2336",
      "input.background": "#1e2336"
    }
  },
  "catppuccin.italicComments": true,
  "catppuccin.italicKeywords": true,
  "catppuccin.boldKeywords": true,
  "catppuccin.syncWithIconPack": true,
  "catppuccin.workbenchMode": "minimal",
  // prevent VSCode from modifying the terminal colors
  "terminal.integrated.minimumContrastRatio": 1,
  // applicable if you use Go, this is an opt-in flag!
  "gopls": {
    "ui.semanticTokens": true
  },
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 1500,
  "files.enableTrash": true,
  "files.associations": {
    // "*.css": "tailwindcss"
  },
  /* ---------------------------------------------------------------------- */
  /*                             Cursor                                     */
  /* ---------------------------------------------------------------------- */
  "workbench.colorTheme": "Catppuccin Macchiato",
  "workbench.sideBar.location": "right",
  "workbench.editor.customLabels.patterns": {
    "**/{layout,page}.tsx": "${dirname} ${filename}"
  },
  "workbench.statusBar.visible": false,
  "window.titleBarStyle": "native",
  "workbench.settings.editor": "json",
  "workbench.editor.showTabs": "single",
  "workbench.editor.tabSizing": "shrink",
  "workbench.list.smoothScrolling": true,
  "workbench.fontAliasing": "auto",
  "workbench.tree.expandMode": "singleClick",
  "workbench.commandPalette.experimental.suggestCommands": true,
  "workbench.editor.editorActionsLocation": "hidden",
  "workbench.activityBar.location": "hidden",
  "breadcrumbs.filePath": "last",
  "workbench.iconTheme": "catppuccin-macchiato",
  "workbench.list.fastScrollSensitivity": 100,
  "workbench.tree.indent": 13,
  "workbench.tree.renderIndentGuides": "none",
  "workbench.tree.enableStickyScroll": false,
  //   "workbench.tree.stickyScrollMaxItemCount": 7,
  //   "workbench.editor.pinnedTabSizing": "compact",
  //   "workbench.editor.tabSizing": "fit",
  "workbench.settings.openDefaultKeybindings": true,
  "workbench.experimental.share.enabled": true,
