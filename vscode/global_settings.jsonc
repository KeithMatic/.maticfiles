  //     "editorBracketHighlight.unexpectedBracket.foreground": "#ff0000"
  //     // "tree.indentGuidesStroke": "#ff7e79",
  //   },
  "workbench.activityBar.iconClickBehavior": "focus",
  "workbench.layoutControl.enabled": false,

  /* ---------------------------------------------------------------------- */
  /*                             animaitons                                 */
  /* ---------------------------------------------------------------------- */
  "animations.Enabled": true,
  "animations.Install-Method": "Custom UI Style",
  "animations.Auto-Install": true,
  "animations.Smooth-Mode": true,
  "animations.CursorAnimationOptions": {
    "Color": "#ffffff",
    "CursorStyle": "block",
    "TrailLength": 8
  },
  "animations.Tabs": "Slide",
  "animations.Active": "Indent",
  /* ---------------------------------------------------------------------- */
  /*                             editor                                     */
  /* ---------------------------------------------------------------------- */
  "editor.fontFamily": "'Lotion Nerd Font','OperatorMonoSSmLig Nerd Font', 'Cascadia Code PL',  'Ellograph CF', MonoLisa, 'Latin Modern Mono', 'GeistMono Nerd Font', 'Comic Code Ligatures'",
  "editor.fontSize": 15,
  "editor.fontWeight": "400",
  "editor.fontLigatures": "'calt', 'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'ss07', 'zero', 'onum', 'cv24'",
  "editor.lineHeight": 1,
  "editor.letterSpacing": 0.6,
  "editor.linkedEditing": true,
  "editor.matchBrackets": "near",
  "editor.lineNumbers": "relative",
  "editor.foldingHighlight": false,
  "editor.stickyScroll.scrollWithEditor": false,
  "editor.stickyScroll.enabled": false,
  "editor.maxTokenizationLineLength": 20000,
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.minimap.autohide": true,
  "editor.minimap.enabled": false,
  "editor.formatOnSave": true,
  "editor.suggest.showStatusBar": true,
  "editor.suggest.preview": true,
  "editor.quickSuggestionsDelay": 0,
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "editor.tabSize": 2,
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "editor.inlineSuggest.showToolbar": "always",
  "editor.snippetSuggestions": "none",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.wordWrap": "bounded",
  "editor.tabCompletion": "on",
  "editor.suggest.showDeprecated": false,
  "editor.parameterHints.cycle": true,
  "editor.occurrencesHighlight": "off",
  "editor.semanticHighlighting.enabled": true,
  "editor.hideCursorInOverviewRuler": false,
  "editor.overviewRulerBorder": false,
  "editor.experimental.asyncTokenization": true,
  "editor.autoClosingComments": "always",
  "editor.autoClosingDelete": "always",
  "editor.codeActions.triggerOnFocusChange": true,
  "editor.experimentalInlineEdit.enabled": true,
  "editor.inlayHints.padding": true,
  "editor.lightbulb.enabled": "off",
  "editor.smoothScrolling": true,
  "editor.stablePeek": true,
  "editor.unicodeHighlight.nonBasicASCII": true,
  "editor.inlineSuggest.fontFamily": "Dank Mono",
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "hidden",
  "editor.scrollbar.verticalScrollbarSize": 1,
  "editor.scrollbar.horizontalScrollbarSize": 3,
  "editor.quickSuggestions": {
    "other": true,
    "comments": false,
    "strings": "on"
  },
  "editor.codeActionsOnSave": {
    "source.sortImports": "explicit",
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit"
  },
  "editor.acceptSuggestionOnEnter": "off",
  "editor.inlineSuggest.enabled": true,
  "editor.stickyTabStops": false,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "editor.guides.highlightActiveIndentation": true,
  "editor.guides.indentation": true,
  "editor.guides.bracketPairsHorizontal": "active",
  "editor.guides.highlightActiveBracketPair": true,
  "editor.renderLineHighlight": "gutter",
  "editor.hover.above": false,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.codeLensFontFamily": "OperatorMonoSSmLig Nerd Font",
  "editor.definitionLinkOpensInPeek": true,
  "editor.foldingImportsByDefault": true,
  "editor.inlayHints.enabled": "onUnlessPressed",
  "editor.inlayHints.fontFamily": "OperatorMonoSSmLig Nerd Font",
  "editor.autoClosingBrackets": "always",
  "editor.language.colorizedBracketPairs": [
    ["<", ">"],
    ["{", "}"],
    ["[", "]"],
    ["(", ")"]
  ],
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "meta.function-call",
          "meta.type.annotation",
          "meta.method.declaration",
          "meta.name.function",
          "support.type.primitive",
          "accessor",
          "comment",
          "keyword.control",
          "keyword.operator.new",
          "keyword.operator.assignment",
          "keyword.operator.arithmetic",
          "storage.modifier",
          "storage.type",
          "support.class",
          "support.constant",
          "support.other",
          "support.type.object.module",
          "entity.name.method",
          "entity.name.section",
          "entity.name.selector",
          "entity.name.function",
          "entity.name.type",
          "entity.other",
          "entity.other.attribute-name",
          "entity.other.inherited-class",
          "variable.other.object",
          "meta.use.rust",
          "meta.function.definition.rust",
          "meta.function.call.rust",
          "meta.type.annotation",
          "entity.name.type.go"
        ],
        "settings": {
          // "fontStyle": "italic"
        }
      },
      {
        "scope": [
          "storage.type.function.arrow",
          "meta.paragraph.markdown",
          "entity.name.type.result.rust",
          "source.rust",
          "other"
        ],
        "settings": {
          // "fontStyle": "italic"
        }
      },
      {
        "scope": [
          // "entity.name.function",
          "variable.object.property",
          "entity.name.type",
          "variable.other.property",
          "meta.definition.variable",
          "meta.definition.function",
          "meta.function.expression",
          "variable.other.readwrite",
          "keyword.operator.namespace.rust",
          // "entity.name.type.rust",
          "variable.other.rust",
          "variable.other",
          "variable.parameter",
          "variable.other.object",
          "entity.name.namespace.rust",
          "entity.name.function.rust",
          "string",
          "entity.name.function.go",
          "constant.numeric",
          "entity.name.type.class",
          "meta.type",
          "meta.type-alias",
          "support.type",
          "entity.name.type"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  /* ---------------------------------------------------------------------- */
  /*                             Cursor                                     */
  /* ---------------------------------------------------------------------- */
  // To improve performance
  "extensions.experimental.affinity": {
    "vscodevim.vim": 1
  },
  "vim.leader": "<space>",
  "vim.cursorStylePerMode.insert": "line",
  "vim.cursorStylePerMode.normal": "block",
  "vim.cursorStylePerMode.visual": "block-outline",
  "vim.highlightedyank.enable": true,
  "vim.easymotionDimBackground": true,
  "vim.overrideCopy": true,
  "vim.usesystemclipboard": true,
  "vim.surround": true,
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.handleKeys": {
    "<C-d>": true,
    "<C-a>": false,
    "<C-f>": false
  },
  "vim.cursorStylePerMode.replace": "underline",
  "vim.foldfix": true,
  "vim.smartRelativeLine": true,
  "vim.sneak": true,
  "vim.targets.enable": true,
  // ============================================= Insert Mode
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "k"],
      "commands": ["workbench.action.files.save"],
      "after": ["<Esc>"]
    },
    {
      "before": ["<c-h>"],
      "commands": ["cursorLeft"]
    },
    {
      "before": ["<c-l>"],
      "commands": ["cursorRight"]
    },
    {
      "before": ["<c-j>"],
      "commands": ["cursorDown"]
    },
    {
      "before": ["<c-k>"],
      "commands": ["cursorUp"]
    }
  ],
  "vim.normalModeKeyBindings": [
    // splits
    {
      "before": ["leader", "|"],
      "commands": [":vsplit"]
    },
    {
      "before": ["leader", "-"],
      "commands": [":split"]
    },
    {
      "before": ["<leader>", ","],
      "commands": ["workbench.action.showAllEditors"]
    }
  ],
  "vim.operatorPendingModeKeyBindings": [
    {
      "before": ["L"],
      "after": ["$"]
    },
    {
      "before": ["H"],
      "after": ["^"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "d"],
      "after": ["d", "d"]
    },
    {
      "before": ["<leader>", "f"],
      "commands": ["workbench.action.showAllEditors"]
    },
    {
      "before": [":"],
      "commands": ["workbench.action.showCommands"]
    },
    {
      "before": ["<C-x>"],
      "commands": [":nohl"]
    },
    {
      "before": ["K"],
      "commands": ["lineBreakInsert"],
      "silent": true
    },
    // switch b/w buffers
    {
      "before": ["<S-h>"],
      "commands": [":bprevious"]
    },
    // Close active editor
    {
      "before": ["<leader>", "c"],
      "commands": ["workbench.action.closeActiveEditor"]
    },
    // splits
    {
      "before": [
        // "leader",
        "|"
      ],
      "commands": [":vsplit"]
    },
    {
      "before": [
        // "leader",
        // "-"
        "\\"
      ],
      "commands": [":split"]
    },
    // panes
    {
      "before": ["<ctrl+h>"],
      "commands": ["workbench.action.focusLeftGroup"]
    },
    {
      "before": ["<ctrl+j>"],
      "commands": ["workbench.action.focusBelowGroup"]
    },
    {
      "before": ["<ctrl+k>"],
      "commands": ["workbench.action.focusAboveGroup"]
    },
    {
      "before": ["<ctrl+l>"],
      "commands": ["workbench.action.focusRightGroup"]
    },
    // FILE TREE
    {
      "before": ["leader", "e"],
      "commands": ["workbench.action.toggleSidebarVisibility"]
    },
    {
      "before": ["leader", "n", "e"],
      "commands": ["workbench.explorer.fileView.focus"]
    },
    // NICE TO HAVE
    {
      "before": ["leader", "t", "h"],
      "commands": ["workbench.action.selectTheme"]
    },
    {
      "before": ["leader", "w"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["leader", "W"],
      "commands": ["workbench.action.files.saveAll"]
    },
    {
      "before": ["leader", "f", "f"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["leader", "f", "w"],
      "commands": ["workbench.action.findInFiles"]
    },
    {
      "before": ["/"],
      "commands": ["actions.find"]
    },
    {
      "before": ["leader", "p"],
      "commands": ["editor.action.formatDocument"]
    },
    {
      "before": ["g", "h"],
      "commands": ["editor.action.showDefinitionPreviewHover"]
    },
    // Start Imported vim keybindings
    {
      "before": ["leader", "r"],
      "commands": ["editor.action.rename"]
    },
    {
      "before": ["leader", "w"],
      "commands": [":w!"]
    },
    {
      "before": ["leader", "q"],
      "commands": [":q!"]
    },
    {
      "before": ["leader", "x"],
      "commands": [":x!"]
    },
    {
      "after": ["g", "g", "V", "G"],
      "before": ["<c-a>"]
    },
    {
      "before": ["<leader>", "k"],
      "commands": ["editor.action.showHover"]
    },
    {
      "before": ["[", "d"],
      "commands": ["editor.action.marker.prev"]
    },
    {
      "before": ["]", "d"],
      "commands": ["editor.action.marker.next"]
    },
    {
      "before": ["<leader>", "c", "a"],
      "commands": ["editor.action.quickFix"]
    },
    // editor folds
    {
      "before": ["<leader>", "z", "c"],
      "commands": ["editor.fold"]
    },
    {
      "before": ["<leader>", "z", "o"],
      "commands": ["editor.unfold"]
    },
    {
      "before": ["<leader>", "z", "a"],
      "commands": ["editor.unfoldAll"]
    },
    // Move to beginning/end of line
    {
      "after": ["^"],
      "before": ["H"]
    },
    {
      "after": ["$"],
      "before": ["L"]
    },
    // End Move to beginning/end of line
    {
      "before": ["leader", "i"],
      "commands": ["extension.toggleBool"]
    },
    {
      "before": ["J"],
      "after": ["5", "j"]
    },
    {
      "before": ["K"],
      "after": ["5", "k"]
    },
    // TERMINAL SHORTCUTS
    //
    {
      "before": ["leader", "t"],
      "commands": ["workbench.action.terminal.toggleTerminal"]
    },
    {
      "before": ["leader", "t", "c"],
      "commands": ["workbench.action.terminal.killActiveTab"]
    },
    {
      // Focus the terminal when the terminal is not focused
      "key": "ctrl+;",
      "command": "workbench.action.terminal.focus"
    },
    /* ------------------------------- Code runner ------------------------------ */
    {
      "before": ["<leader>", "r"],
      "commands": ["code-runner.run"]
    },
    {
      "before": ["<leader>", "r", "r"]
    },
    {
      "before": ["<leader>", "r", "s"],
      "commands": ["code-runner.stop"]
    }
  ],
  // ============================================= Visual Mode
  "vim.visualModeKeyBindings": [
    // Stay in visual mode while indenting
    {
      "before": ["<"],
      "commands": ["editor.action.outdentLines"]
    },
    {
      "before": [">"],
      "commands": ["editor.action.indentLines"]
    },
    // Move selected lines while staying in visual mode
    {
      "before": ["J"],
      "commands": ["editor.action.moveLinesDownAction"]
    },
    {
      "before": ["K"],
      "commands": ["editor.action.moveLinesUpAction"]
    },
    // toggle comment selection
    {
      "before": ["leader", "/"],
      "commands": ["editor.action.commentLine"]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["p"],
      "after": ["p", "g", "v", "y"]
    }
  ],
